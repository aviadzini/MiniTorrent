<Window x:Class="MiniTorrentClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiniTorrentClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="loginB" Content="Login" HorizontalAlignment="Left" Margin="136,171,0,0" VerticalAlignment="Top" Width="75" Click="loginB_Click"/>
        <Label x:Name="label" Content="Username:" HorizontalAlignment="Left" Margin="60,58,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Password:" HorizontalAlignment="Left" Margin="60,106,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="usernameTB" HorizontalAlignment="Left" Height="23" Margin="136,61,0,0" TextWrapping="Wrap" Text="Enter username" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="passwordTB" HorizontalAlignment="Left" Height="23" Margin="136,106,0,0" TextWrapping="Wrap" Text="Enter password" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label2" Content="Login" HorizontalAlignment="Left" Margin="218,21,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
